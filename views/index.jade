extends layout

block content
    .row
        #frontLeftRed(data-bind='if: frontLeft() >= 3')
            .col-lg-3.col-md-6
                .panel.panel-red
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Left
                                div Adjust Fit

        #frontLeftYellow(data-bind='if: frontLeft() == 2')
            .col-lg-3.col-md-6
                .panel.panel-yellow
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Left
                                div Adjust Fit

        #frontLeftGreen(data-bind='if: frontLeft() == 1')
            .col-lg-3.col-md-6
                .panel.panel-green
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Left
                                div Ready

        #frontRightRed(data-bind='visible: frontRight() > 3 ')
            .col-lg-3.col-md-6
                .panel.panel-red
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Right
                                div Adjust Fit

        #frontRightYellow(data-bind='visible: frontRight() == 2 ')
            .col-lg-3.col-md-6
                .panel.panel-yellow
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Right
                                div Adjust Fit

        #frontRightYellow(data-bind='visible: frontRight() == 1 ')
            .col-lg-3.col-md-6
