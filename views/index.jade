                .panel.panel-yellow
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Left Ear
                                div Adjust Fit

        #leftEarRed(data-bind='visible: leftEar() == 1 ')
            .col-lg-3.col-md-6
                .panel.panel-green
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Left Ear
                                div Ready

        #rightEarGreen(data-bind='visible: rightEar() > 3 ')
            .col-lg-3.col-md-6
                .panel.panel-red
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Right Ear
                                div Adjust Fit

