doctype html(lang='en')
head
    meta(charset='utf-8')

    meta(http-equiv='X-UA-Compatible', content='IE=edge')

    meta(name='viewport', content='width=device-width, initial-scale=1')

    meta(name='description', content='')

    meta(name='author', content='')

    title Radiance
    // Bootstrap Core CSS
    link(href='css/bootstrap.min.css', rel='stylesheet')
    // Custom CSS
    link(href='css/sb-admin.css', rel='stylesheet')
    // Morris Charts CSS
    link(href='css/plugins/morris.css', rel='stylesheet')
    // Custom Fonts
    link(href='font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')

    link(href='css/radiant.css', rel='stylesheet')
body
    #wrapper
        // Navigation
        nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
            // Brand and toggle get grouped for better mobile display
            .navbar-header
                button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
                    span.sr-only Toggle navigation

                    span.icon-bar

                    span.icon-bar

                    span.icon-bar

                a.navbar-brand(href='index.html') Radiance

            // Sidebar Menu Items - These collapse to the responsive navigation menu on small screens
            .collapse.navbar-collapse.navbar-ex1-collapse
                ul.nav.navbar-nav.side-nav.radiance-nav
                    li.active
                        a(href='/')
                            i.fa.fa-fw.fa-bar-chart-o
                            span &nbsp Dashboard

                    li
                        a(href='/controls')
                            i.fa.fa-fw.fa-sliders
                            span &nbsp Controls

                    li
                        div.light-icon
                            #light1.i.fa.fa-circle
                            #light2.i.fa.fa-circle
                            #light3.i.fa.fa-circle
                            #light4.i.fa.fa-circle


            // /.navbar-collapse

        #page-wrapper
            .container-fluid
                .row
                    .col-lg-3.col-md-6
                        .panel.panel-red
                            .panel-heading
                                .row
                                    .col-xs-3
                                        i.fa.fa-exclamation-triangle.fa-5x
                                    .col-xs-9.text-right
                                        .huge Front Left
                                        div Adjust Fit

                    .col-lg-3.col-md-6
                        .panel.panel-red
                            .panel-heading
                                .row
                                    .col-xs-3
                                        i.fa.fa-exclamation-triangle.fa-5x
                                    .col-xs-9.text-right
                                        .huge Front Right
                                        div Adjust Fit

                    .col-lg-3.col-md-6
                        .panel.panel-red
                            .panel-heading
                                .row
                                    .col-xs-3
                                        i.fa.fa-exclamation-triangle.fa-5x
                                    .col-xs-9.text-right
                                        .huge Left Ear
                                        div Adjust Fit

                    .col-lg-3.col-md-6
                        .panel.panel-red
                            .panel-heading
                                .row
                                    .col-xs-3
                                        i.fa.fa-exclamation-triangle.fa-5x
                                    .col-xs-9.text-right
                                        .huge Right Ear
                                        div Adjust Fit


                    .row
                    .col-lg-6
                        .panel.panel-primary
                            .panel-heading
                                h3.panel-title
                                    i.fa.fa-bar-chart-o
                                        span &nbsp Mellow Rating

                            .panel-body
                                .flot-chart
                                    #mellow-graph.flot-chart-content

                    .col-lg-6
                        .panel.panel-primary
                            .panel-heading
                                h3.panel-title
                                    i.fa.fa-bar-chart-o
                                        span &nbsp Concentration Rating

                            .panel-body
                                .flot-chart
                                    #concentration-graph.flot-chart-content

                    .col-lg-6
                        .panel.panel-green
                            .panel-heading
                                h3.panel-title
                                    i.fa.fa-bar-chart-o
                                        span &nbsp Blink

                            .panel-body
                                .flot-chart
                                    #blink-graph.flot-chart-content

                    .col-lg-6
                        .panel.panel-green
                            .panel-heading
                                h3.panel-title
                                    i.fa.fa-bar-chart-o
                                        span &nbsp Jaw Clench

                            .panel-body
                                .flot-chart
                                    #clench-graph.flot-chart-content

                // /.row
            // /.container-fluid
        // /#page-wrapper
    // /#wrapper
    // jQuery
    script(src='js/jquery.js')
    // Bootstrap Core JavaScript
    script(src='js/bootstrap.min.js')
    // Morris Charts JavaScript
    script(src='js/plugins/morris/raphael.min.js')

    script(src='js/plugins/morris/morris.min.js')

    script(src='js/plugins/morris/morris-data.js')
