                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Front Right
                                div Ready

        #leftEarGreen(data-bind='visible: leftEar() > 3 ')
            .col-lg-3.col-md-6
                .panel.panel-red
                    .panel-heading
                        .row
                            .col-xs-3
                                i.fa.fa-exclamation-triangle.fa-5x
                            .col-xs-9.text-right
                                .huge Left Ear
                                div Adjust Fit

        #leftEarYellow(data-bind='visible: leftEar() == 2 ')
