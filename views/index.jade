        .row
        .col-lg-6
            .panel.panel-primary
                .panel-heading
                    h3.panel-title
                        i.fa.fa-bar-chart-o
                            span &nbsp Mellow Rating

                .panel-body
                    .flot-chart
                        #mellow-graph.flot-chart-content

        .col-lg-6
            .panel.panel-primary
                .panel-heading
                    h3.panel-title
                        i.fa.fa-bar-chart-o
                            span &nbsp Concentration Rating

                .panel-body
                    .flot-chart
                        #concentration-graph.flot-chart-content

        .col-lg-6
            .panel.panel-green
                .panel-heading
                    h3.panel-title
                        i.fa.fa-bar-chart-o
